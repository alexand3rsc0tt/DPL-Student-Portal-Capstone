<!DOCTYPE html>
<html>
  <head>
    <title>Capstone</title>
    <%= stylesheet_link_tag    'application' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="wrapper"> <!-- body wrapper -->
      <%= render '/layouts/nav' %>
      <div class="page-wrapper-header"> <!-- Header main content wrapper -->
        <%= render '/layouts/breadcrumbs' %>
      </div>
      <div class="page-wrapper"> <!-- Main content wrapper -->
        <div class="container-fluid">

          <%= yield %>
      </div>
    </div>
  </body>
  <footer>
    <p class="Copyright">&copy; DevPoint Labs 2014</p>
  </footer>
</html>
